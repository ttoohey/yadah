{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "node": "18"
        }
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "allExtensions": true
      }
    ]
  ],
  "plugins": ["@babel/plugin-proposal-export-default-from"],
  "sourceMaps": "inline",
  "comments": false
}
