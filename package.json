    "prettier:check": "npm run --quiet --workspaces --if-present -- prettier --check",
    "test:data-manager": "npm test --workspace @yadah/data-manager --",
    "test:dedupe-mixin": "npm test --workspace @yadah/dedupe-mixin --",
    "test:objection-iterator": "npm test --workspace @yadah/objection-iterator --",
    "test:objection-scope": "npm test --workspace @yadah/objection-scope --",
    "test:service-critical-section": "npm test --workspace @yadah/service-critical-section --",
    "test:service-listener": "npm test --workspace @yadah/service-listener --",
    "test:service-model": "npm test --workspace @yadah/service-model --",
    "test:subsystem-context": "npm test --workspace @yadah/subsystem-context --",
    "test:subsystem-knex": "npm test --workspace @yadah/subsystem-knex --",
    "test:subsystem-message-queue": "npm test --workspace @yadah/subsystem-message --",
    "test:subsystem-logger": "npm test --workspace @yadah/subsystem-logger --",
    "test:subsystem-pubsub": "npm test --workspace @yadah/subsystem-pubsub --",
    "test:subsystem-schedule": "npm test --workspace @yadah/service-schedule --",
    "test": "npm test --quiet --workspaces --if-present --",
    "workspace:data-manager": "npm run --workspace @yadah/data-manager --",
    "workspace:dedupe-mixin": "npm run --workspace @yadah/dedupe-mixin --",
    "workspace:objection-listener": "npm run --workspace @yadah/objection-listener --",
    "workspace:objection-scope": "npm run --workspace @yadah/objection-scope --",
    "workspace:service-critical-section": "npm run --workspace @yadah/critical-section --",
    "workspace:service-listener": "npm run --workspace @yadah/service-listener --",
    "workspace:service-model": "npm run --workspace @yadah/service-model --",
    "workspace:subsystem-context": "npm run --workspace @yadah/subsystem-context --",
    "workspace:subsystem-knex": "npm run --workspace @yadah/subsystem-knex --",
    "workspace:subsystem-logger": "npm run --workspace @yadah/subsystem-logger --",
    "workspace:subsystem-message-queue": "npm run --workspace @yadah/subsystem-message-queue --",
    "workspace:subsystem-pubsub": "npm run --workspace @yadah/subsystem-pubsub --",
    "workspace:subsystem-schedule": "npm run --workspace @yadah/service-schedule --"
