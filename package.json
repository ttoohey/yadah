  "workspaces": [
    "packages/data-manager",
    "packages/dedupe-mixin",
    "packages/depcheck",
    "packages/objection-iterator",
    "packages/objection-scope",
    "packages/service-critical-section",
    "packages/service-listener",
    "packages/service-model",
    "packages/subsystem-context",
    "packages/subsystem-knex",
    "packages/subsystem-logger",
    "packages/subsystem-message-queue",
    "packages/subsystem-pubsub",
    "packages/subsystem-schedule",
    "packages/yadah"
  ],
  "devDependencies": {
    "concurrently": "^7.2.1",
    "depcheck": "^1.4.3",
    "husky": "^8.0.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3"
  }
